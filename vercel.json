    "VITE_DEFAULT_LANGUAGE": "ar",
    "VITE_AI_ENABLED": "true",
    "VITE_AI_FALLBACK_ENABLED": "true",
    "VITE_AI_CIRCUIT_BREAKER_THRESHOLD": "3",
    "VITE_CHART_WORKER_ENABLED": "true",
    "VITE_PWA_CACHE_ENABLED": "true",
    "VITE_ANALYTICS_ENABLED": "true"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options", 
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "/consultant",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://wlmljniorublcadvorvf.supabase.co/functions/v1/$1"
    }
  ]
